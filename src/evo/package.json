	"scripts": {
		"ng": "nx",
		"postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
		"start": "nx serve",
		"build": "nx build",
		"test": "nx test",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"antlr": "yarn run antlr-task && yarn run antlr-queryengine && yarn run antlr-fix",
		"antlr-queryengine": "yarn run antlr-queryengine-lexer && yarn run antlr-queryengine-parser && yarn run antlr-fix",
		"antlr-queryengine-lexer": "antlr4ts -visitor ./libs/code-editors/src/lib/language-services/query-engine/QueryEngineLexer.g4 -o ./libs/code-editors/src/lib/language-services/query-engine/.antlr",
		"antlr-queryengine-parser": "antlr4ts -visitor ./libs/code-editors/src/lib/language-services/query-engine/QueryEngineParser.g4 -o ./libs/code-editors/src/lib/language-services/query-engine/.antlr",
		"antlr-task": "antlr4ts -visitor ./libs/code-editors/src/lib/language-services/task/Task.g4 -o ./libs/code-editors/src/lib/language-services/task/.antlr && yarn run antlr-fix",
		"antlr-fix": "node ./antlr-fix.js && yarn format"
	},
