{
	"extends": ["../../.eslintrc.json"],
	"ignorePatterns": ["!**/*", "**/.antlr/*.*"],
	"overrides": [
		{
			"files": ["*.ts"],
			"extends": ["../../.eslintrc.angular.json"],
			"parserOptions": {
